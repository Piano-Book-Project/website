{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:user && npm run build:admin",
    "build:user": "cd apps/user && npm install && npm run build",
    "build:admin": "cd apps/admin && npm install && npm run build",
    "dev": "concurrently \"npm run dev:user\" \"npm run dev:admin\"",
    "dev:user": "cd apps/user && npm run dev",
    "dev:admin": "cd apps/admin && npm run dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@prisma/client": "^6.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trpc/server": "^11.4.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.12.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "react-youtube": "^10.1.0",
    "sass": "^1.89.2"
  }
}
