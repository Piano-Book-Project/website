{
  "name": "piano-book-admin",
  "private": true,
  "version": "1.0.0",
  "description": "김츄츄의 피아노책 어드민 대시보드",
  "type": "module",
  "homepage": "https://piano-book-project.github.io/website/",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"yarn --cwd ../backend dev\" \"yarn dev\"",
    "build": "tsc -b && vite build",
    "build:all": "yarn --cwd ../backend build && yarn build",
    "start:all": "concurrently \"yarn --cwd ../backend start\" \"yarn preview\"",
    "build:prod": "NODE_ENV=production vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "predeploy": "yarn build:prod",
    "deploy": "GITHUB_PAGES=true yarn build:prod && gh-pages -d dist",
    "prepare": "husky install",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^7.2.0",
    "@mui/x-date-pickers": "^7.2.0",
    "@react-spring/web": "^10.0.1",
    "@tanstack/react-query": "^5.83.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.22.2",
    "axios": "^1.10.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^12.23.3",
    "lodash-es": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-grid-layout": "^1.5.2",
    "react-hot-toast": "^2.4.1",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^7.6.3",
    "react-virtualized": "^9.22.6",
    "recharts": "^3.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-grid-layout": "^1.3.5",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}